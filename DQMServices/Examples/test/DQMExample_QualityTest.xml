<TESTSCONFIGURATION>

<QTEST name="EfficiencyInRange" activate="true">
	<TYPE>ContentsYRange</TYPE>	
	<PARAM name="ymin">0.8</PARAM>
	<PARAM name="ymax">1</PARAM>
	<PARAM name="useEmptyBins">false</PARAM>
	<PARAM name="error">0.7</PARAM>
	<PARAM name="warning">0.9</PARAM>
</QTEST>

<LINK name="Physics/TopTest/*myEfficiency*">
	<TestName activate="true">EfficiencyInRange</TestName>
</LINK>

</TESTSCONFIGURATION>
