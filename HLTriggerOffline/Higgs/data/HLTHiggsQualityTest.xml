	<TestName activate="true">efficiencyCheck</TestName>
</LINK>

<LINK name="HLT/Higgs/HZZ/Eff*">
	<TestName activate="true">efficiencyCheck</TestName>
</LINK>

<LINK name="HLT/Higgs/H2tau/Eff*">
