<use name="FWCore/ParameterSet"/>
<use name="FWCore/MessageLogger"/>
<use name="SimG4Core/Physics"/>
<use name="geant4core"/>
<use name="clhep"/>
<use name="heppdt"/>
<use name="boost"/>
<library file="*.cc" name="SimG4CorePhysicsListsPlugins">
  <use name="SimG4Core/PhysicsLists"/>
  <flags EDM_PLUGIN="1"/>
</library>
<iftool name="adept">
  <library file="adept/*.cc" name="SimG4CorePhysicsListsAdeptPlugins">
    <use name="SimG4Core/PhysicsLists"/>
    <use name="adept"/>
    <flags EDM_PLUGIN="1"/>
  </library>
</iftool>