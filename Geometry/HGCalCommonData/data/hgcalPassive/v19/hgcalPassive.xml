    <Numeric name="ShiftTop"         value="[shiftTop]"/>
    <Numeric name="ShiftBottom"      value="[shiftBottom]"/>
    <Vector name="SlopeBottom" type="numeric" nEntries="4">
      0, 0, 0, 0</Vector>
