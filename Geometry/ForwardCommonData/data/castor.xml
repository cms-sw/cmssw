  <ZSection rMin="165.790*mm"  rMax="165.7905*mm"   z="202.2*mm" />
 </Polyhedra>
<!-- layer    7 copies (5.+2)*sqrt(2)+0.60=10.5  (7.07+2.83+0.60) -->
 <Polyhedra name="CASL" numSide="1" startPhi="0.0*deg" deltaPhi="45.0*deg" >
