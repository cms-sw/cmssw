  <ZSection rMin="168.200*mm"  rMax="168.2005*mm"   z="1451.7*mm" />
 </Polyhedra>
<!-- read-out channel  with StainlessSteel size  18 copies -->
 <Polyhedra name="CASR" numSide="1" startPhi="0.0*deg" deltaPhi="45.0*deg" >
