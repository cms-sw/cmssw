  <ZSection rMin="168.200*mm"  rMax="168.2005*mm"   z="202.2*mm" />
 </Polyhedra>
<!-- reduced rMin,rMax by (1.mm (StainlessSteel)/tg(22.5))= 2.41mm-->
<!-- the special X,Y shifts are needed  -->
<!-- the positions: x = 2.41*cos22.5 = 1.30, y=...sin...=0.54  1 copy     -->
 <Polyhedra name="CARR" numSide="1" startPhi="0.0*deg" deltaPhi="45.0*deg" >
  <ZSection rMin="37.090*mm"   rMax="37.0905*mm"    z="0.0*mm" />
