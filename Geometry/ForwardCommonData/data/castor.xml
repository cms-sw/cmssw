  <ZSection rMin="168.200*mm"  rMax="168.2005*mm"   z="1451.7*mm" />
 </Polyhedra>
<!-- to have octant StainlessSteel skeleton to support WQ plates  -->
<!-- CASS sector - Air, 8 copies with deltaPhi=45.0  -->
 <Polyhedra name="CASS" numSide="1" startPhi="0.0*deg" deltaPhi="45.0*deg" >
  <ZSection rMin="39.500*mm"   rMax="39.5005*mm"    z="0.0*mm" />
