  <ZSection rMin="165.790*mm"  rMax="165.7905*mm"   z="139.2*mm" />
 </Polyhedra>
<!-- fiber layer   1 copy 2.43sqrt(2)=3.43  -->
 <Polyhedra name="CASA" numSide="1" startPhi="0.0*deg" deltaPhi="45.0*deg" >
  <ZSection rMin="37.090*mm"   rMax="37.0905*mm"    z="0.0*mm" />
