		<Trd1 dx1="8.505*cm" dx2="5.291*cm" dy1="3.9*cm" dy2="3.9*cm" dz="3.075*cm" name="BOXB"/>
		<Polycone name="VTUB" startPhi="0*deg" deltaPhi="360*deg">
