	<SolidSection label="ionpump.xml">
		<Polycone name="FULLTUB" startPhi="0*deg" deltaPhi="360*deg">
			<ZSection z="0*fm" rMin="0*fm" rMax="1.9*cm"/>
			<ZSection z="40*mm" rMin="0*fm" rMax="1.9*cm"/>
			<ZSection z="40*mm" rMin="0*fm" rMax="3.5*cm"/>
