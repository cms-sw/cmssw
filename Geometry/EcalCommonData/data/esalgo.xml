    </PosPart>
    <PosPart copyNumber="1">
      <rParent name="esalgo:SF"/>
      <rChild name="esalgo:SFRA"/>
      <rRotation name="rotations:000D"/>
      <Translation x="0*fm" y="0*fm" z="[escon:PRESH_Z_TOTAL]/2-[escon:P28]-[escon:P29]"/>
