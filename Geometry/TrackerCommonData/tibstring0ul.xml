<?xml version="1.0"?>
<DDDefinition xmlns="http://www.cern.ch/cms/DDL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.cern.ch/cms/DDL ../../DDLSchema/DDLSchema.xsd">

<ConstantsSection label="tibstring0ul.xml" eval="true">
 <Constant name="zero"         value="0.0*fm"/>
 <Constant name="MotherCableL" value="66.60*cm"/>
 <Constant name="CoolL"        value="67.50*cm"/>
 <Constant name="Det0Z"        value="-59.33735*cm"/>
 <Constant name="Det1Z"        value="-38.27895*cm"/>
 <Constant name="Det2Z"        value="-14.81155*cm"/>
 <Constant name="ICC0Z"        value="-48.810*cm"/>
 <Constant name="ICC1Z"        value="-26.550*cm"/>
 <Constant name="ICC2Z"        value="-3.020*cm"/>
 <Constant name="Shift"        value="[tibstring0c:StringUpPlsL]/2"/>
</ConstantsSection>

<SolidSection label="tibstring0ul.xml">
 <Box name="TIBString0UpMin1"            dx="[tibmodpar:ModuleDx]"    
      dy="[tibmodpar:ModuleDy]"          dz="[tibstring0c:StringUpMinL]/2"/>
 <Box name="TIBString0UpMin2"            dx="[tibstringpar:CoolTubeW]/2"    
      dy="[tibstringpar:CoolTubeD]/2"    dz="[tibstring0c:StringUpMinL]/2"/>
 <Box name="TIBString0UpMinCable"        dx="[tibstringpar:MotherCableW]/2"    
      dy="[tibstringpar:MotherCableT]/2" dz="[MotherCableL]/2"/>
 <Tubs name="TIBString0UpMinCoolS"       rMin="[zero]"      
       rMax="[tibstringpar:CoolTubeD]/2" dz="[CoolL]/2"
       startPhi="0*deg"                  deltaPhi="360*deg"/>
 <Tubs name="TIBString0UpMinCoolW"       rMin="[zero]"      
       rMax="[tibstringpar:CoolTubeD]/2" dz="[tibstringpar:CoolTubeW]/2"
       startPhi="0*deg"                  deltaPhi="360*deg"/>
 </SolidSection>

<LogicalPartSection label="tibstring0ul.xml">
 <LogicalPart name="TIBString0UpMin1" category="unspecified">
  <rSolid name="TIBString0UpMin1"/>
  <rMaterial name="materials:Air"/>
 </LogicalPart>
 <LogicalPart name="TIBString0UpMin2" category="unspecified">
  <rSolid name="TIBString0UpMin2"/>
  <rMaterial name="materials:Air"/>
 </LogicalPart>
 <LogicalPart name="TIBString0UpMinCable" category="unspecified">
  <rSolid name="TIBString0UpMinCable"/>
  <rMaterial name="tibmaterial:TIB_mother_cable"/>
 </LogicalPart>
 <LogicalPart name="TIBString0UpMinCoolS" category="unspecified">
  <rSolid name="TIBString0UpMinCoolS"/>
  <rMaterial name="tibmaterial:TIB_mod_cool"/>
 </LogicalPart>
 <LogicalPart name="TIBString0UpMinCoolW" category="unspecified">
  <rSolid name="TIBString0UpMinCoolW"/>
  <rMaterial name="tibmaterial:TIB_mod_cool"/>
 </LogicalPart>
</LogicalPartSection>

<PosPartSection label="tibstring0ul.xml">
 <PosPart copyNumber="8">
   <rParent name="tibstring0ul:TIBString0UpMin1"/>
   <rChild name="tibstringpar:TIBCCUM"/>
  <Translation x="[zero]"   y="[zero]"  z="(-[tibstringpar:CCUMZ]+[Shift])" />
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibstring0ul:TIBString0UpMin2"/>
   <rChild name="tibstring0ul:TIBString0UpMinCable"/>
  <Translation x="[zero]"   y="-[tibstringpar:MotherCableY]"  
               z="-([tibstring0c:StringUpMinL]-[MotherCableL])/2" />
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibstring0ul:TIBString0UpMin2"/>
   <rChild name="tibstring0ul:TIBString0UpMinCoolS"/>
  <Translation x="-[tibstringpar:CoolTubeSep]/2"   y="[zero]"  
               z="-([tibstring0c:StringUpMinL]-[CoolL])/2" />
 </PosPart>
 <PosPart copyNumber="2">
   <rParent name="tibstring0ul:TIBString0UpMin2"/>
   <rChild name="tibstring0ul:TIBString0UpMinCoolS"/>
  <Translation x="[tibstringpar:CoolTubeSep]/2"    y="[zero]"  
               z="-([tibstring0c:StringUpMinL]-[CoolL])/2" />
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibstring0ul:TIBString0UpMin2"/>
   <rChild name="tibstring0ul:TIBString0UpMinCoolW"/>
  <rRotation name="tibstringpar:90XD"/>
  <Translation x="[zero]"   y="[zero]"  z="([CoolL]-
               ([tibstring0c:StringUpMinL]-[tibstringpar:CoolTubeD])/2)" />
 </PosPart>
</PosPartSection>

<Algorithm name="track:DDTrackerZPosAlgo">
  <rParent name="tibstring0ul:TIBString0UpMin1"/>
  <String name="ChildName"    value="tibmodule0b:TIBModule0B"/>
  <Numeric name="StartCopyNo" value="1"/>
  <Numeric name="IncrCopyNo"  value="1" />
  <Vector name="ZPositions" type="numeric" nEntries="3">
  ([Det0Z]+[Shift]), ([Det1Z]+[Shift]), ([Det2Z]+[Shift]) </Vector>
  <Vector name="Rotations" type="string" nEntries="3">
  tibstringpar:180D, tibstringpar:180D, tibstringpar:180D</Vector>
</Algorithm>

<Algorithm name="track:DDTrackerZPosAlgo">
  <rParent name="tibstring0ul:TIBString0UpMin1"/>
  <String name="ChildName"    value="tibstringds:TIBICCBoxDS"/>
  <Numeric name="StartCopyNo" value="2"/>
  <Numeric name="IncrCopyNo"  value="1" />
  <Vector name="ZPositions" type="numeric" nEntries="3">
  ([ICC0Z]+[Shift]), ([ICC1Z]+[Shift]), ([ICC2Z]+[Shift]) </Vector>
  <Vector name="Rotations" type="string" nEntries="3">
  tibstringpar:NULL, tibstringpar:NULL, tibstringpar:NULL</Vector>
</Algorithm>
 
</DDDefinition>
