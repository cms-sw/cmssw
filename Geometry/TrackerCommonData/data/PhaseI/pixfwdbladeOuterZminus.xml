 <PosPart copyNumber="1">
  <rParent name="pixfwdbladeOuterZminus:PixelForwardBlade"/>
  <rChild name="pixfwdbladeOuterZminus:PixelForwardBladeBase"/>
  <Translation x="0"  y="0"  z="0" />
 </PosPart>
 <PosPart copyNumber="1">
  <rParent name="pixfwdbladeOuterZminus:PixelForwardBladeBase"/>
  <rChild name="pixfwdbladeOuterZminus:TPGBladeBase"/>
  <Translation x="0"  y="0"  z="0" />
 </PosPart>
 <PosPart copyNumber="1">
  <rParent name="pixfwdbladeOuterZminus:PixelForwardBladeBase"/>
  <rChild name="pixfwdbladeOuterZminus:CarbonFiberSkin"/>
  <Translation x="0"  y="0"  z="[TPGBladeBaseT]/2+[CarbonFiberSkinT]/2" />
 </PosPart>
 <PosPart copyNumber="2">
  <rParent name="pixfwdbladeOuterZminus:PixelForwardBladeBase"/>
  <rChild name="pixfwdbladeOuterZminus:CarbonFiberSkin"/>
  <Translation x="0"  y="0"  z="-[TPGBladeBaseT]/2-[CarbonFiberSkinT]/2" />
 </PosPart>

 <PosPart copyNumber="1">
  <rParent name="pixfwdbladeOuterZminus:PixelForwardBlade"/>
  <rChild name="pixfwdbladeOuterZminus:PixelForwardModule"/>
  <Translation x="[ModuleX]+[DetectorShiftX]"  y="[ModuleY]"  z="[BladeBaseT]/2+[ModuleT]/2" />
