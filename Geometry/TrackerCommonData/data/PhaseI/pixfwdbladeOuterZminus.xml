  <rParent name="pixfwdbladeOuterZminus:PixelForwardModule"/>
  <rChild name="pixfwdbladeOuterZminus:PixelForwardWafer"/>
  <Translation x="0"  y="0"  z="-[ModuleT]/2+[AdhesiveFilmT]+[ROChipT]+[BumpBondT]+[WaferT]/2" />
 </PosPart>

 <PosPart copyNumber="1">
