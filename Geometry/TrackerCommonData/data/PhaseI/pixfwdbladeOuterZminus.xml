  <rChild name="pixfwdbladeOuterZminus:PixelForwardROChip"/>
  <Translation x="0" y="0" z="-[BumpBondT]/2" /> <!-- -[ROChipT]/2-[BumpBondT]/2+[ROChipT]/2 -->
