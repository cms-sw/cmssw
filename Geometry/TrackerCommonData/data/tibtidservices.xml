		</LogicalPart>
                <LogicalPart name="TIBTIDServiceDisk1" category="unspecified">
                        <rSolid name="ServiceDisk1"/>
                        <rMaterial name="tibtidcommonmaterial:TIBTID_ServiceDisk1"/>
