<?xml version="1.0"?>
<DDDefinition xmlns="http://www.cern.ch/cms/DDL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.cern.ch/cms/DDL ../../DDLSchema/DDLSchema.xsd">

<ConstantsSection label="tibstring0ur.xml" eval="true">
 <Constant name="zero"         value="0.0*fm"/>
 <Constant name="MotherCableL" value="53.00*cm"/>
 <Constant name="CoolL"        value="70.00*cm"/>
 <Constant name="Det0Z"        value="8.78485*cm"/>
 <Constant name="Det1Z"        value="32.29835*cm"/>
 <Constant name="Det2Z"        value="55.68355*cm"/>
 <Constant name="ICC0Z"        value="20.540*cm"/>
 <Constant name="ICC1Z"        value="43.990*cm"/>
 <Constant name="ICC2Z"        value="65.880*cm"/>
 <Constant name="Shift"        value="-[tibstring0c:StringUpMinL]/2"/>
</ConstantsSection>

<SolidSection label="tibstring0ur.xml">
 <Box name="TIBString0UpPls1"            dx="[tibmodpar:ModuleDx]"    
      dy="[tibmodpar:ModuleDy]"          dz="[tibstring0c:StringUpPlsL]/2"/>
 <Box name="TIBString0UpPls2"            dx="[tibstringpar:CoolTubeW]/2"    
      dy="[tibstringpar:CoolTubeD]/2"    dz="[tibstring0c:StringUpPlsL]/2"/>
 <Box name="TIBString0UpPlsCable"        dx="[tibstringpar:MotherCableW]/2"    
      dy="[tibstringpar:MotherCableT]/2" dz="[MotherCableL]/2"/>
 <Tubs name="TIBString0UpPlsCoolS"       rMin="[zero]"      
       rMax="[tibstringpar:CoolTubeD]/2" dz="[CoolL]/2"
       startPhi="0*deg"                  deltaPhi="360*deg"/>
 <Tubs name="TIBString0UpPlsCoolW"       rMin="[zero]"      
       rMax="[tibstringpar:CoolTubeD]/2" dz="[tibstringpar:CoolTubeW]/2"
       startPhi="0*deg"                  deltaPhi="360*deg"/>
 </SolidSection>

<LogicalPartSection label="tibstring0ur.xml">
 <LogicalPart name="TIBString0UpPls1" category="unspecified">
  <rSolid name="TIBString0UpPls1"/>
  <rMaterial name="materials:Air"/>
 </LogicalPart>
 <LogicalPart name="TIBString0UpPls2" category="unspecified">
  <rSolid name="TIBString0UpPls2"/>
  <rMaterial name="materials:Air"/>
 </LogicalPart>
 <LogicalPart name="TIBString0UpPlsCable" category="unspecified">
  <rSolid name="TIBString0UpPlsCable"/>
  <rMaterial name="tibmaterial:TIB_mother_cable"/>
 </LogicalPart>
 <LogicalPart name="TIBString0UpPlsCoolS" category="unspecified">
  <rSolid name="TIBString0UpPlsCoolS"/>
  <rMaterial name="tibmaterial:TIB_mod_cool"/>
 </LogicalPart>
 <LogicalPart name="TIBString0UpPlsCoolW" category="unspecified">
  <rSolid name="TIBString0UpPlsCoolW"/>
  <rMaterial name="tibmaterial:TIB_mod_cool"/>
 </LogicalPart>
</LogicalPartSection>

<PosPartSection label="tibstringur.xml">
 <PosPart copyNumber="1">
   <rParent name="tibstring0ur:TIBString0UpPls1"/>
   <rChild name="tibstringpar:TIBCCUM"/>
  <Translation x="[zero]"   y="[zero]"  z="([tibstringpar:CCUMZ]+[Shift])" />
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibstring0ur:TIBString0UpPls2"/>
   <rChild name="tibstring0ur:TIBString0UpPlsCable"/>
  <Translation x="[zero]"   y="-[tibstringpar:MotherCableY]"  
               z="([tibstring0c:StringUpPlsL]-[MotherCableL])/2" />
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibstring0ur:TIBString0UpPls2"/>
   <rChild name="tibstring0ur:TIBString0UpPlsCoolS"/>
  <Translation x="-[tibstringpar:CoolTubeSep]/2"   y="[zero]"  
               z="([tibstring0c:StringUpPlsL]-[CoolL])/2" />
 </PosPart>
 <PosPart copyNumber="2">
   <rParent name="tibstring0ur:TIBString0UpPls2"/>
   <rChild name="tibstring0ur:TIBString0UpPlsCoolS"/>
  <Translation x="[tibstringpar:CoolTubeSep]/2"    y="[zero]"  
               z="([tibstring0c:StringUpPlsL]-[CoolL])/2" />
 </PosPart>
 <PosPart copyNumber="1">
   <rParent name="tibstring0ur:TIBString0UpPls2"/>
   <rChild name="tibstring0ur:TIBString0UpPlsCoolW"/>
  <rRotation name="tibstringpar:90XD"/>
  <Translation x="[zero]"   y="[zero]"  z="-([CoolL]-
               ([tibstring0c:StringUpPlsL]-[tibstringpar:CoolTubeD])/2)" />
 </PosPart>
</PosPartSection>

<Algorithm name="track:DDTrackerZPosAlgo">
  <rParent name="tibstring0ur:TIBString0UpPls1"/>
  <String name="ChildName"    value="tibmodule0b:TIBModule0B"/>
  <Numeric name="StartCopyNo" value="4"/>
  <Numeric name="IncrCopyNo"  value="1" />
  <Vector name="ZPositions" type="numeric" nEntries="3">
  ([Det0Z]+[Shift]), ([Det1Z]+[Shift]), ([Det2Z]+[Shift]) </Vector>
  <Vector name="Rotations" type="string" nEntries="3">
  tibstringpar:180D, tibstringpar:180D, tibstringpar:180D</Vector>
</Algorithm>

<Algorithm name="track:DDTrackerZPosAlgo">
  <rParent name="tibstring0ur:TIBString0UpPls1"/>
  <String name="ChildName"    value="tibstringds:TIBICCBoxDS"/>
  <Numeric name="StartCopyNo" value="5"/>
  <Numeric name="IncrCopyNo"  value="1" />
  <Vector name="ZPositions" type="numeric" nEntries="3">
  ([ICC0Z]+[Shift]), ([ICC1Z]+[Shift]), ([ICC2Z]+[Shift]) </Vector>
  <Vector name="Rotations" type="string" nEntries="3">
  tibstringpar:NULL, tibstringpar:NULL, tibstringpar:NULL</Vector>
</Algorithm>
 
</DDDefinition>
