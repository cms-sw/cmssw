      dimension kproj(mamx,mamx),ktarg(mamx,mamx)
     *,lproj(mamx),ltarg(mamx)
      equivalence (kproj(1,1),kremn(1,1,1)),(ktarg(1,1),kremn(1,1,2))
      equivalence (lproj(1),lremn(1,1)),(ltarg(1),lremn(1,2))
      double precision gammaV
