      common/col4/bk(kollmx),bhpr(npommx,kollmx),bkx(kollmx)
     *,bky(kollmx)
      double precision PomInck,xxnuc,xzcutpar
      common/col7/zparremn(kollmx,2),PomInck(kollmx),xzcutpar(kollmx)
