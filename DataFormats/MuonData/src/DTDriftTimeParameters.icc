/*** Auxiliary functions ***/

unsigned short THIS_CLASS::MB_DT_Check_boundaries(double distime, double alpha, double by, double bz, short ifl) const {
  unsigned short status = 1;
  std::string name = "MB_DT_drift_time";

  if (ifl < 0) {
    name = "MB_DT_drift_distance";
    if (distime < 0) {
#ifdef MB_DT_DEBUG
      printf("*** Warning, %s:  drift time is negative\n", name);
#endif
      status = 0;
    }
  } else {
    if (fabs(distime) > DT_Cell_HalfWidth) {
#ifdef MB_DT_DEBUG
      if (ifl == 0)
        printf("*** Warning, %s:  distance to the wire is out of range [-%d,%d] mm\n",
               name,
               DT_Cell_HalfWidth,
               DT_Cell_HalfWidth);
      else
        printf("*** Warning, %s:  x-coordinate is out of range [0,%d] mm\n", name, DT_Cell_Width);
#endif
      status = 0;
    }
  }

  if (alpha < alpha_value[0] || alpha > alpha_value[N_alpha - 1]) {
#ifdef MB_DT_DEBUG
    printf("*** Warning, %s:  angle is out of range [%.0f,%.0f] degrees\n",
           name,
           alpha_value[0],
           alpha_value[N_alpha - 1]);
#endif
    status = 0;
  }

  if (by < By_value[0] || by > By_value[N_By - 1]) {
#ifdef MB_DT_DEBUG
    printf("*** Warning, %s:  By field is out of range [%.2f,%.2f]\n", name, By_value[0], By_value[N_By - 1]);
#endif
    status = 0;
  }

  if (bz < Bz_value[0] || bz > Bz_value[N_Bz - 1]) {
#ifdef MB_DT_DEBUG
    printf("*** Warning, %s:  Bz field is out of range [%.2f,%.2f]\n", name, Bz_value[0], Bz_value[N_Bz - 1]);
#endif
    status = 0;
  }

  return (status);
}

void THIS_CLASS::MB_DT_Get_grid_values(
    double Var, unsigned short *pi, unsigned short *pj, short Initial, unsigned short N, const double *Values) const {
  unsigned short i, iValue, jValue;

  iValue = N - 1;
  jValue = N - 2;

  for (i = Initial; i < N; i++) {
    if (Var <= Values[i]) {
      iValue = i;
      jValue = (Var != Values[i]) ? i - 1 : i;
      break;
    }
  }

  if (fabs(Values[iValue] - Var) <= fabs(Var - Values[jValue])) {
    *pi = iValue;
    *pj = jValue;
  } else {
    *pi = jValue;
    *pj = iValue;
  }
}

void THIS_CLASS::MB_DT_Get_grid_points(double alpha,
                                       double by,
                                       double bz,
                                       unsigned short *p_alpha,
                                       unsigned short *p_By,
                                       unsigned short *p_Bz,
                                       unsigned short *q_alpha,
                                       unsigned short *q_By,
                                       unsigned short *q_Bz) const {
  MB_DT_Get_grid_values(fabs(alpha), p_alpha, q_alpha, 4, N_alpha, alpha_value);
  MB_DT_Get_grid_values(by, p_By, q_By, 0, N_By, By_value);
  MB_DT_Get_grid_values(bz, p_Bz, q_Bz, 0, N_Bz, Bz_value);

  if (alpha < 0) {
    *p_alpha = N_alpha - 1 - *p_alpha;
    *q_alpha = N_alpha - 1 - *q_alpha;
  }
}

/*** Multidimensional linear interpolation ***/

double THIS_CLASS::MB_DT_MLInterpolation(double *al, double *by, double *bz, double *f) const {
  double q1, q2, q3, p1, p2, p3;
  double fx11, fx21, fxy1, fx12, fx22, fxy2, fxyz;

  q1 = (al[1] != al[2]) ? (al[0] - al[1]) / (al[2] - al[1]) : 0;  // alpha
  q2 = (by[1] != by[2]) ? (by[0] - by[1]) / (by[2] - by[1]) : 0;  // By
  q3 = (bz[1] != bz[2]) ? (bz[0] - bz[1]) / (bz[2] - bz[1]) : 0;  // Bz

  p1 = 1 - q1;
  p2 = 1 - q2;
  p3 = 1 - q3;

  fx11 = p1 * f[0] + q1 * f[4];
  fx21 = p1 * f[2] + q1 * f[6];
  fxy1 = p2 * fx11 + q2 * fx21;

  fx12 = p1 * f[1] + q1 * f[5];
  fx22 = p1 * f[3] + q1 * f[7];
  fxy2 = p2 * fx12 + q2 * fx22;

  fxyz = p3 * fxy1 + q3 * fxy2;

  return (fxyz);
}

double THIS_CLASS::MB_DT_sigma_t_m(double dist, double *par) const {
  double x = fabs(dist);  // the parametrisations are symmetric under 'distance'

  if (x > 20.5)
    x = 20.5;

  return (par[6] * x);
}

double THIS_CLASS::MB_DT_sigma_t_p(double dist, double *par) const {
  double x2, x = fabs(dist);  // the parametrisations are symmetric under 'distance'

  if (x > 20.5)
    x = 20.5;

  x2 = x * x;

  if (x <= par[5]) {
    return (par[0] + par[1] * x + par[2] * x2);
  }
  return (par[3] + par[4] * x);
}
