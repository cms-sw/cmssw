<TESTSCONFIGURATION>

	<!--
		Definition of all quality tests for L1 HfBitCounts trigger object from
		all the involved systems.
	-->

    <QTEST name="L1EmulatorHfBitCountsGctErrorFlagQT">
        <TYPE>ContentsXRange</TYPE>
        <PARAM name="warning">0.99</PARAM>
        <PARAM name="error">0.95</PARAM>
        <PARAM name="xmin">0.0</PARAM>
        <PARAM name="xmax">1.0</PARAM>
    </QTEST>


    <LINK name="*Stage1Layer2expert/HFCnts/HFCntsErrorFlag">
        <TestName activate="true">L1EmulatorHfBitCountsGctErrorFlagQT</TestName>
    </LINK>


</TESTSCONFIGURATION>
