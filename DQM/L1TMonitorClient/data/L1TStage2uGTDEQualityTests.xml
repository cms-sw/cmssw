<TESTSCONFIGURATION>

  <!-- uGT Data vs Emulator -->

  <!-- Mismatch Summary QTest -->

  <QTEST name="uGTDE_CentralBxMismatchRatio">
    <TYPE>ContentsYRange</TYPE>	
    <PARAM name="ymin">0.00</PARAM>
    <PARAM name="ymax">1.e-4</PARAM>
    <PARAM name="error">0.95</PARAM>
    <PARAM name="warning">0.99</PARAM>
    <PARAM name="useEmptyBins">1</PARAM>
  </QTEST>
  
  <LINK name="L1TEMU/L1TdeStage2uGT/dataEmulMismatchRatio_CentralBX">
      <TestName activate="true">uGTDE_CentralBxMismatchRatio</TestName>
  </LINK>

</TESTSCONFIGURATION>
