<TESTSCONFIGURATION>


<QTEST name="DeadChannels_RPCTF_2D">
         <TYPE>DeadChannel</TYPE>
         <PARAM name="error">0.99</PARAM>
         <PARAM name="warning">0.50</PARAM>
         <PARAM name="threshold">0</PARAM>
</QTEST>

<QTEST name="HotChannels_RPCTF_2D">
         <TYPE>NoisyChannel</TYPE>
         <PARAM name="error">0.80</PARAM>
         <PARAM name="warning">0.70</PARAM>
         <PARAM name="tolerance">10</PARAM>
         <PARAM name="neighbours">1</PARAM>
</QTEST>

<LINK name="*RPCTF_muons_eta_phi_bx0">
        <TestName activate="false">HotChannels_RPCTF_2D</TestName>
        <TestName activate="false">DeadChannels_RPCTF_2D</TestName>
</LINK>


</TESTSCONFIGURATION>
