<TESTSCONFIGURATION>

	<!--
		Definition of all quality tests for L1 ETT trigger object from all the
		involved systems.
	-->

    <QTEST name="L1EmulatorEttGctErrorFlagQT">
        <TYPE>ContentsXRange</TYPE>
        <PARAM name="warning">0.99</PARAM>
        <PARAM name="error">0.95</PARAM>
        <PARAM name="xmin">0.0</PARAM>
        <PARAM name="xmax">1.0</PARAM>
    </QTEST>


    <LINK name="*GCTexpert/ET/ETErrorFlag">
        <TestName activate="true">L1EmulatorEttGctErrorFlagQT</TestName>
    </LINK>


</TESTSCONFIGURATION>
