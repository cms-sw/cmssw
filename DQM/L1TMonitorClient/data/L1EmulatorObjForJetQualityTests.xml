<TESTSCONFIGURATION>

	<!--
		Definition of all quality tests for L1 ForJet trigger object from all
		the involved systems.
	-->


    <QTEST name="L1EmulatorForJetGctErrorFlagQT">
        <TYPE>ContentsXRange</TYPE>
        <PARAM name="warning">0.99</PARAM>
        <PARAM name="error">0.95</PARAM>
        <PARAM name="xmin">0.0</PARAM>
        <PARAM name="xmax">1.0</PARAM>
    </QTEST>


    <LINK name="*GCTexpert/ForJet/ForJetErrorFlag">
        <TestName activate="true">L1EmulatorForJetGctErrorFlagQT</TestName>
    </LINK>

</TESTSCONFIGURATION>
