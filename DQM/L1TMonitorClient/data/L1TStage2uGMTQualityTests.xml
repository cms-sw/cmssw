    <PARAM name="warning">0.5</PARAM>
    <PARAM name="minEntries">10</PARAM>
  </QTEST>
  
  <LINK name="*/L1TStage2uGMT/*/ugmtBMTFBX">
      <TestName activate="true">uGMT_BMTFBXPeakAtBX0</TestName>
      <TestName activate="true">uGMT_BMTFBXMeanAtBX0</TestName>
  </LINK>


