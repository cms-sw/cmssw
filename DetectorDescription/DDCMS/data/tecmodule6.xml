		<Constant name="SideFrameRtheta" value="-0.040*rad"/>
		<Constant name="SideFrameRHeight" value="210.8*mm"/>
		<Constant name="SideFrameRWidth" value="13.9*mm"/>
		<Constant name="SideFrameLtheta" value="0.040*rad"/>
