#! /bin/bash
#
# generate HLT tables from master table in ConfDB
#

MASTER="/dev/CMSSW_3_11_1/HLT_fix3"             # no version, take the latest one
TARGET="/online/collisions/2011/5e32/v4"        # where to store the online-compliant menu
TABLES="HLT"

source subtables.sh
createSubtables "hltdev" "$MASTER" "$TARGET" "$TABLES"
