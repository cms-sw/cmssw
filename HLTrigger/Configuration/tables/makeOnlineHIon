#! /bin/bash
#
# generate HLT tables from master table in ConfDB
#

MASTER="/dev/CMSSW_7_5_0/HLT"                       # no version, take the latest one
TARGET="/online/collisions/2015/HeavyIons/v1.0/HLT" # where to store the online-compliant menu
TABLES="online_hion"

source subtables.sh
createSubtables "v2/offline" "$MASTER" "$TARGET" "$TABLES"
